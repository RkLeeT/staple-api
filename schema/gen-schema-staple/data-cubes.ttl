@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sdmx-attribute:  <http://purl.org/linked-data/sdmx/2009/attribute#> .

schema:Thing a rdfs:Class ;
    rdfs:comment "Anything." .

qb:DataSet a rdfs:Class;
    rdfs:label "Data set";
    rdfs:comment "Represents a collection of observations, possibly organized into various slices, conforming to some common dimensional structure." ;
    rdfs:subClassOf schema:Thing .

qb:Observation a rdfs:Class;
    rdfs:label "Observation";
    rdfs:comment "A single observation in the cube, may have one or more associated measured values" ;
    rdfs:subClassOf schema:Thing .

qb:dataSet a rdf:Property, owl:FunctionalProperty;
    rdfs:label "data set";
    rdfs:comment "indicates the data set of which this observation is a part";
    schema:domainIncludes qb:Observation;
    schema:rangeIncludes qb:DataSet .

qb:observation a rdf:Property;
    rdfs:label "observation";
    rdfs:comment "indicates a observation contained within this slice of the data set";
    schema:domainIncludes qb:ObservationGroup;
    schema:rangeIncludes qb:Observation .

qb:ObservationGroup a rdfs:Class;
    rdfs:label "Observation Group";
    rdfs:comment "A, possibly arbitrary, group of observations." ;
    rdfs:subClassOf schema:Thing .

qb:Slice a rdfs:Class;
    rdfs:subClassOf qb:ObservationGroup ;
    rdfs:label "Slice";
    rdfs:comment "Denotes a subset of a DataSet defined by fixing a subset of the dimensional values, component properties on the Slice" .

qb:ComponentSet a rdfs:Class ;
    rdfs:label "Component set";
    rdfs:comment "Abstract class of things which reference one or more ComponentProperties" ;
    rdfs:subClassOf schema:Thing .

qb:slice a rdf:Property;
    rdfs:label "slice";
    rdfs:comment "Indicates a subset of a DataSet defined by fixing a subset of the dimensional values";
    schema:domainIncludes qb:DataSet;
    schema:rangeIncludes qb:Slice .

sdmx-attribute:unitMeasure a rdf:Property  ;
    rdfs:label "Unit of Measure" ;
    rdfs:comment "The unit in which the data values are measured." ;
    schema:domainIncludes qb:DataSet ;
    schema:domainIncludes qb:Observation ;
    schema:rangeIncludes skos:Concept .

qb:DimensionProperty a rdfs:Class;
    rdfs:label "Dimension property";
    rdfs:comment "The class of components which represent the dimensions of the cube" ;
    rdfs:subClassOf qb:ComponentProperty .

qb:MeasureProperty a rdfs:Class;
    rdfs:label "Measure property";
    rdfs:comment "The class of components which represent the measured value of the phenomenon being observed";
    rdfs:subClassOf qb:ComponentProperty .

qb:ComponentProperty a rdfs:Class ;
    rdfs:label "Component property (abstract)";
    rdfs:comment "Abstract super-property of all properties representing dimensions, attributes or measures" ;
    rdfs:subClassOf schema:Thing .
 
qb:DataStructureDefinition a rdfs:Class;
    rdfs:label "Data structure definition";
    rdfs:comment "Defines the structure of a DataSet or slice" ;
    rdfs:subClassOf qb:ComponentSet .

qb:structure a rdf:Property, owl:FunctionalProperty;
    rdfs:label "structure";
    rdfs:comment "indicates the structure to which this data set conforms";
    schema:domainIncludes qb:DataSet;
    schema:rangeIncludes qb:DataStructureDefinition .

qb:component a rdf:Property;
    rdfs:label "component specification";
    rdfs:comment "indicates a component specification which is included in the structure of the dataset";
    schema:domainIncludes qb:DataStructureDefinition;
    schema:rangeIncludes qb:ComponentSpecification .

qb:ComponentSpecification a rdfs:Class ;
    rdfs:label "Component specification";
    rdfs:comment "Used to define properties of a component (attribute, dimension etc) which are specific to its usage in a DSD." ;
    rdfs:subClassOf qb:ComponentSet .

qb:SliceKey a rdfs:Class;
    rdfs:label "Slice key";
    rdfs:comment "Denotes a subset of the component properties of a DataSet which are fixed in the corresponding slices";
    rdfs:subClassOf qb:ComponentSet .

qb:sliceStructure a rdf:Property, owl:FunctionalProperty;
    rdfs:label "slice structure";
    rdfs:comment "indicates the sub-key corresponding to this slice";
    schema:domainIncludes qb:Slice;
    schema:rangeIncludes  qb:SliceKey .

qb:sliceKey a rdf:Property;
    rdfs:label "slice key";
    rdfs:comment "indicates a slice key which is used for slices in this dataset";
    schema:domainIncludes qb:DataStructureDefinition;
    schema:rangeIncludes  qb:SliceKey .

qb:dimension a rdf:Property ;
    rdfs:label "dimension" ;
    rdfs:comment "An alternative to qb:componentProperty which makes explicit that the component is a dimension" ;
    schema:domainIncludes qb:ComponentSpecification ;
    schema:rangeIncludes qb:DimensionProperty .

qb:measure a rdf:Property;
    rdfs:label "measure";
    rdfs:comment "An alternative to qb:componentProperty which makes explicit that the component is a measure";
    schema:domainIncludes qb:ComponentSpecification ;
    schema:rangeIncludes qb:MeasureProperty;
